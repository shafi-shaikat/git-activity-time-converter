{
  "manifest_version": 2,

  "name": "Git Activity Time Converter",
  "description": "This extension adds real time beside Git time ago",
  "version": "2",
  "permissions": ["tabs", "*://*.github.com/*", "*://*.gitlab.witopia.net/*", "*://*.gitfs.net//*"],
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*", "*://*.gitlab.witopia.net/*", "*://*.gitfs.net//*"],
      "js": ["thirdParty/jquery.min.js","thirdParty/moment.min.js","script.js"]
    }
  ]
}
