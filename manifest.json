{
  "manifest_version": 2,

  "name": "Gitfs Time ago Converter",
  "description": "This extension adds real time beside Gitfs time ago",
  "version": "1.1",
  "permissions": ["tabs", "*://*.gitfs.net/*", "*://*.gitlab.witopia.net/*"],
  "content_scripts": [
    {
      "matches": ["*://*.gitfs.net/*", "*://*.gitlab.witopia.net/*"],
      "js": ["thirdParty/jquery.min.js","thirdParty/moment.min.js","script.js"]
    }
  ]
}